{
  "name": "revolt-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@rebass/grid": "^6.0.0",
    "@sentry/browser": "^4.5.2",
    "@types/async-retry": "^1.4.1",
    "@types/color": "^3.0.0",
    "@types/country-list": "^2.1.0",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "24.0.18",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/mongoose": "^5.3.1",
    "@types/node": "12.7.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-redux": "^7.1.0",
    "@types/react-router": "^4.0.32",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-stripe-elements": "^1.1.9",
    "@types/react-textarea-autosize": "^4.3.4",
    "@types/react-truncate": "^2.3.3",
    "@types/rebass__grid": "^6.0.5",
    "@types/recharts": "^1.1.21",
    "@types/redux-promise-middleware": "0.0.11",
    "@types/styled-components": "^4.1.8",
    "@types/superagent": "^3.8.4",
    "approximate-number": "^2.0.0",
    "async-retry": "^1.2.3",
    "copy-to-clipboard": "^3.0.8",
    "country-list": "^2.2.0",
    "cypress": "^3.4.1",
    "cypress-jest-adapter": "^0.1.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fast-deep-equal": "^2.0.1",
    "lodash.uniqby": "^4.7.0",
    "material-icons-react": "^1.0.4",
    "moment": "^2.24.0",
    "prettier": "^1.18.2",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.8",
    "react-google-login": "^5.0.5",
    "react-gtm-module": "^2.0.8",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-router-last-location": "^2.0.1",
    "react-scripts": "3.1.2",
    "react-scrolllock": "^3.0.2",
    "react-spinners": "^0.5.13",
    "react-stripe-elements": "^2.0.1",
    "react-styled-frame": "^1.1.0",
    "react-textarea-autosize": "^7.1.0",
    "recharts": "^1.7.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-promise-middleware": "^5.1.1",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.1.3",
    "styled-reset": "^1.7.0",
    "superagent": "^4.0.0-beta.5",
    "typescript": "3.6.2"
  },
  "devDependencies": {
    "@types/socket.io-client": "^1.4.32"
  },
  "scripts": {
    "dev": "yarn start",
    "start": "PORT=8080 EXTEND_ESLINT=true HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "impliedStrict": true,
        "classes": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "settings": {
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "typescript": {}
      }
    },
    "rules": {
      "no-debugger": 0,
      "no-alert": 0,
      "no-await-in-loop": 0,
      "no-use-before-define": 0,
      "no-case-declarations": 0,
      "react/destructuring-assignment": 0,
      "camelcase": 0,
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-restricted-syntax": [
        2,
        "ForInStatement",
        "LabeledStatement",
        "WithStatement"
      ],
      "no-unused-vars": [
        0,
        {
          "ignoreSiblings": true,
          "argsIgnorePattern": "res|next|^err"
        }
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "all"
        }
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "no-unused-expressions": [
        2,
        {
          "allowTaggedTemplates": true
        }
      ],
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "no-console": 0,
      "import/prefer-default-export": 0,
      "import": 0,
      "func-names": 0,
      "space-before-function-paren": 0,
      "comma-dangle": 0,
      "max-len": 0,
      "import/extensions": 0,
      "no-underscore-dangle": 0,
      "consistent-return": 0,
      "no-undef": 0,
      "import/no-cycle": 0,
      "typescript-eslint/no-unused-vars": 0,
      "react/display-name": 1,
      "react/no-array-index-key": 0,
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "react/sort-comp": 0,
      "react/prefer-stateless-function": 0,
      "react/no-unescaped-entities": 0,
      "jsx-a11y/accessible-emoji": 0,
      "react/require-default-props": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".tsx"
          ]
        }
      ],
      "radix": 0,
      "no-shadow": [
        2,
        {
          "hoist": "all",
          "allow": [
            "resolve",
            "reject",
            "done",
            "next",
            "err",
            "error"
          ]
        }
      ],
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 100,
          "semi": false
        }
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "plugins": [
      "prettier",
      "react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
